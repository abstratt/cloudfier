<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/mdd/api"
            class="com.abstratt.kirra.mdd.rest.LegacyKirraRESTServlet">
      </servlet>
      <servlet
          alias="/mdd/api-v2"
          class="com.abstratt.kirra.mdd.rest.KirraRESTOnMDDServlet"/>
   </extension>
   <extension
         point="com.abstratt.resman.features">
      <feature
            provider="com.abstratt.kirra.mdd.rest.RESTExternalServiceProvider">
      </feature>
    <feature
            provider="com.abstratt.kirra.mdd.rest.KirraRESTActorSelectorProvider">
      </feature>
   </extension>
</plugin>
